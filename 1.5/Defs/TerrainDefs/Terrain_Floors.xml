	<!-- ================= Misc Tiles ================= -->
	<TerrainDef ParentName="FloorBase">
		<defName>RB_SterileGrateTile</defName>
		<label>sterile grate tile</label>
		<renderPrecedence>243</renderPrecedence>
		<description>Sterile grate tiles with special cleanliness-enhancing properties. Extra-clean rooms improve outcomes in hospitals and research labs. This tile is very slow to build, but very quick to clean.</description>
		<texturePath>Terrain/Surfaces/GrateTile</texturePath>
		<color>(0.71, 0.71, 0.71)</color>
		<pollutionTintColor>(0.95, 0.95, 0.93, 1)</pollutionTintColor>
		<isPaintable>true</isPaintable>
		<statBases>
			<WorkToBuild>1800</WorkToBuild>
			<Cleanliness>0.6</Cleanliness>
			<CleaningTimeFactor>0.3</CleaningTimeFactor>
